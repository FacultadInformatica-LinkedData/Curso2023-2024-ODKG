map:jc_000 rr:child "BARRIO" ;
	rr:parent "BARRIO" .

map:jc_001 rr:child "DISTRITO" ;
	rr:parent "DISTRITO" .

map:map_District_000 rml:logicalSource map:source_002 ;
	rdf:type rr:TriplesMap ;
	rdfs:label "District" ;
	rr:predicateObjectMap map:pom_007, map:pom_008 ;
	rr:subjectMap map:s_002 .

map:map_Neighborhood1_000 rml:logicalSource map:source_001 ;
	rdf:type rr:TriplesMap ;
	rdfs:label "Neighborhood1" ;
	rr:predicateObjectMap map:pom_004, map:pom_005, map:pom_006 ;
	rr:subjectMap map:s_001 .

map:map_Park_000 rml:logicalSource map:source_000 ;
	rdf:type rr:TriplesMap ;
	rdfs:label "Park" ;
	rr:predicateObjectMap map:pom_000, map:pom_001, map:pom_002, map:pom_003 ;
	rr:subjectMap map:s_000 .

map:om_000 rdf:type rr:ObjectMap ;
	rr:constant vama:Park ;
	rr:termType rr:IRI .

map:om_001 rml:reference "ID_PARQUE" ;
	rdf:type rr:ObjectMap ;
	rr:termType rr:Literal .

map:om_002 rml:reference "PARQUE" ;
	rdf:type rr:ObjectMap ;
	rr:termType rr:Literal .

map:om_003 rdf:type rr:ObjectMap ;
	rr:constant vama:Neighborhood1 ;
	rr:termType rr:IRI .

map:om_004 rml:reference "BARRIO" ;
	rdf:type rr:ObjectMap ;
	rr:termType rr:Literal .

map:om_005 rdf:type rr:ObjectMap ;
	rr:constant vama:District ;
	rr:termType rr:IRI .

map:om_006 rml:reference "DISTRITO" ;
	rdf:type rr:ObjectMap ;
	rr:termType rr:Literal .

map:om_007 rdf:type rr:ObjectMap ;
	rr:joinCondition map:jc_000 ;
	rr:parentTriplesMap map:map_Neighborhood1_000 .

map:om_008 rdf:type rr:ObjectMap ;
	rr:joinCondition map:jc_001 ;
	rr:parentTriplesMap map:map_District_000 .

map:pm_000 rdf:type rr:PredicateMap ;
	rr:constant rdf:type .

map:pm_001 rdf:type rr:PredicateMap ;
	rr:constant property:id_park .

map:pm_002 rdf:type rr:PredicateMap ;
	rr:constant property:name .

map:pm_003 rdf:type rr:PredicateMap ;
	rr:constant property:isInNeighborhood .

map:pm_004 rdf:type rr:PredicateMap ;
	rr:constant rdf:type .

map:pm_005 rdf:type rr:PredicateMap ;
	rr:constant property:name .

map:pm_006 rdf:type rr:PredicateMap ;
	rr:constant property:isInDistrict .

map:pm_007 rdf:type rr:PredicateMap ;
	rr:constant rdf:type .

map:pm_008 rdf:type rr:PredicateMap ;
	rr:constant property:name .

map:pom_000 rdf:type rr:PredicateObjectMap ;
	rr:objectMap map:om_000 ;
	rr:predicateMap map:pm_000 .

map:pom_001 rdf:type rr:PredicateObjectMap ;
	rr:objectMap map:om_001 ;
	rr:predicateMap map:pm_001 .

map:pom_002 rdf:type rr:PredicateObjectMap ;
	rr:objectMap map:om_002 ;
	rr:predicateMap map:pm_002 .

map:pom_003 rdf:type rr:PredicateObjectMap ;
