:om_008 rdf:type rr:ObjectMap ;
	rr:template "http://madridwastemanagement.org/group01/data/WasteType/WasteType_{WasteType}" ;
	rr:termType rr:IRI .

:pom_009 rdf:type rr:PredicateObjectMap ;
	rr:predicateMap :pm_009 ;
	rr:objectMap :om_009 .

:pm_009 rdf:type rr:PredicateMap ;
